{
  "include": [
    "src",
  ],
  "exclude": [
    "dist",
    "node_modules",
  ],
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "target": "ES2022",
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "./dist",
  },
}
